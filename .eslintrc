{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "disable-line": 0,
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "no-console": 0,
    "comma-dangle": "off"
  }
}
